{
  "name": "xoiatix/laravel-onboard",
  "description": "Laravel package to help track onboarding steps.",
  "homepage": "https://github.com/xoiatix/laravel-onboard",
  "license": "MIT",
  "keywords": [
    "xoiatix",
    "laravel",
    "laravel onboard"
  ],
  "authors": [
    {
      "name": "Liam Norris",
      "email": "hello@xoiatix.com"
    }
  ],
  "require": {
    "php": "^8.3",
    "laravel/framework": "^11.0"
  },
  "require-dev": {
    "orchestra/testbench": "^9.0",
    "pestphp/pest": "^2.0",
    "pestphp/pest-plugin-laravel": "^2.0"
  },
  "autoload": {
    "psr-4": {
      "Xoiatix\\Onboard\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Xoiatix\\Onboard\\Tests\\": "tests"
    }
  },
  "scripts": {
    "test": "vendor/bin/pest"
  },
  "config": {
    "sort-packages": true,
    "allow-plugins": {
      "pestphp/pest-plugin": true
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Xoiatix\\Onboard\\ServiceProvider"
      ]
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
